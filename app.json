{
  "expo": {
    "name": "Tap&Track",
    "slug": "tapandtrack",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icone-app.png",
    "newArchEnabled": true,
    "scheme": "tap-and-track-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icone-app.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "The app records your location while you use it to ensure your safety and help you find your work regarding your location.",
          "locationAlwaysPermission": "The app records your location while you use it to ensure your safety and help you find your work regarding your location.",
          "locationWhenInUsePermission": "The app records your location while you use it to ensure your safety and help you find your work regarding your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "4",
      "supportsTablet": true,
      "bundleIdentifier": "com.pedroivoss.tapandtrackapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icone-app.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 1,
      "package": "com.pedroivoss.tapandtrackapp",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    }
  }
}
